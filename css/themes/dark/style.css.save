    .scrollup{
      opacity:0.3;
      position:fixed;
      bottom:50px;
      right:100px;
      display:none;     
      text-align: center;
      line-height: 50px;
      font-size: 50px;
      -webkit-transition: color 200ms ease-in-out;
      -moz-transition:    color 200ms ease-in-out;
      -o-transition:      color 200ms ease-in-out;
      -ms-transition:   color 200ms ease-out;
      display: none;
      overflow: hidden;
    }
    
