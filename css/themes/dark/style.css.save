  .form-control {
     display:block;
     width:100%;
     height:38px;
     padding:8px 12px;
     font-size:14px;
     font-family: 'Questrial', sans-serif; 'lucida console', monospace;
     line-height:1.42857143;
     color:#c2b000;
     background-color:#606060;
     background-image:none;
     border:1px solid #181717;
     border-radius:4px;
     -webkit-box-shadow:inset 0 1px 1px rgba(0,0,0,0.075);
     box-shadow:inset 0 1px 1px rgba(0,0,0,0.075);
     -webkit-transition:border-color ease-in-out 1.5s,-webkit-box-shadow ease-in-out 1.5s, background-color ease-in-out 2s;
     -o-transition:border-color ease-in-out 1.5s,box-shadow ease-in-out 1.5s, background-color ease-in-out 2s;
transition: border-color ease-in-out 1.5s,box-shadow ease-in-out 1.5s, background-color ease-in-out 2s;
  }
  .form-control:focus {
     border-color:#353eff66;
     outline:0;
   -webkit-box-shadow:1px 0px 18px 3px rgba(0, 3, 252, 0.81);
   box-shadow:-3px 0px 8px 2px #ffe700;
  background-color: #000000;
  }
  
  .form-control::-moz-placeholder {
   color:#ddd;
   opacity:0.8;
  }
  .form-control:-ms-input-placeholder {
   color:#dddddd;
   opacity:0.8;
  }
  .form-control::-webkit-input-placeholder {
   color:#dddddd;
   opacity:0.8;
  }
    
